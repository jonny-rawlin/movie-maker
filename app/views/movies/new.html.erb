<h1>Add your new movie here!</h1>

<br>
<form action="/movies" method="POST">
  <label for="movie_name"><strong>Name</strong></label>
  <input type="text" id="movie_name" name="movie[name]"></input>
  <br>
  <br>
  <label for="movie_genre"><strong>Genre</strong></label>
  <input type="text" id="movie_genre" name="movie[genre]"></input>
  <br>
  <br>
  <label for="movie_release_date"><strong>Release Date</strong></label>
  <input type="text" id="movie_release_date" name="movie[release_date]"></input>
  <br>
  <br>
  <label for="movie_director"><strong>Director</strong></label>
  <input type="text" id="movie_director" name="movie[director]"></input>
  <br>
  <br>
  <label for="movie_actor_one"><strong>Actor One</strong></label>
  <input type="text" id="movie_actor_one" name="movie[actor_one]"></input>
  <br>
  <br>
  <label for="movie_actor_two"><strong>Actor Two</strong></label>
  <input type="text" id="movie_actor_two" name="movie[actor_two]"></input>
  <br>
  <br>
  <input type="submit">
</form>
